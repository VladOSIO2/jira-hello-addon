{
  "key": "${addon.key}",
  "baseUrl": "${addon.base-url}",
  "name": "Jira Hello Addon",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": [
    "read",
    "write",
    "act_as_user"
  ],
  "modules": {
    "generalPages": [
      {
        "key": "hello-world-page-jira",
        "location": "system.top.navigation.bar",
        "name": {
          "value": "Hello World"
        },
        "url": "/hello-world"
      }
    ]
  }
}
